#include <stdio.h>
#include <string.h>

int main() {
    char str[100],ch;
    int i, j, len,count=0;

    fgets(str, sizeof(str), stdin);
    scanf("%c",&ch);

    // get the length of the string
    len = strlen(str);

     
    for (i = 0; i < len; i++) {

            if (str[i] == ch) { //check input character and string element same or not
                 
                count++; //if same then count
            }

    }

     

    printf("%c is %d times",ch, count);

    return 0;

}

